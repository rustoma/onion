{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/onion/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/onion/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/onion",
  "projects": {
    "onion-api": {
      "type": "application",
      "root": "apps/onion-api",
      "entryFile": "main",
      "sourceRoot": "apps/onion-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/onion-api/tsconfig.app.json"
      }
    },
    "onion-scraper": {
      "type": "application",
      "root": "apps/onion-scraper",
      "entryFile": "main",
      "sourceRoot": "apps/onion-scraper/src",
      "compilerOptions": {
        "tsConfigPath": "apps/onion-scraper/tsconfig.app.json"
      }
    },
    "db": {
      "type": "library",
      "root": "libs/db",
      "entryFile": "index",
      "sourceRoot": "libs/db/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db/tsconfig.lib.json"
      }
    }
  }
}